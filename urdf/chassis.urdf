<?xml version="1.0"?>
<robot name="chassis">
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.954 0.700 0.1"/>
            </geometry>

            <origin xyz="0 0 0" rpy="0 0 0"/>

            <material name="blue">
                <color rgba="0 .5 .8 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.954 0.700 0.1"/>>
            </geometry>
        </collision>
        <inertial>
            <mass value="19.31"/>
            <origin xyz="-0.02282 0.002773 0.008173"/>
            <inertia ixx="4.041e-1" ixy="7.617e-3" ixz="-1.317e-2" iyy="3.322e-1"
                     iyz="7.985e-2" izz="1.8e-1"/>
        </inertial>
    </link>

    <link name="right_front_wheel">
        <visual>
            <geometry>
                <cylinder length="0.072" radius="0.1265"/>
            </geometry>
            <!--            <origin xyz="0 0 0" rpy="${M_PI/2} 0 0"/>-->
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        <collision>
            <!--            <origin xyz="0 0 0" rpy="${M_PI/2} 0 0"/>-->
            <geometry>
                <cylinder length="0.072" radius="0.1265"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.59"/>
            <inertia ixx="1.416e-3" ixy="0.0" ixz="0.0" iyy="2.749e-3" iyz="0.0" izz="1.416e-3"/>
        </inertial>
    </link>


    <link name="right_back_wheel">
        <visual>
            <geometry>
                <cylinder length="0.072" radius="0.1265"/>
            </geometry>
            <!--            <origin xyz="0 0 0" rpy="${M_PI/2} 0 0"/>-->
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        <collision>
            <!--            <origin xyz="0 0 0" rpy="${M_PI/2} 0 0"/>-->
            <geometry>
                <cylinder length="0.072" radius="0.1265"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.59"/>
            <inertia ixx="1.416e-3" ixy="0.0" ixz="0.0" iyy="2.749e-3" iyz="0.0" izz="1.416e-3"/>
        </inertial>
    </link>


    <link name="left_front_wheel">
        <visual>
            <geometry>
                <cylinder length="0.072" radius="0.1265"/>
            </geometry>
            <!--            <origin xyz="0 0 0" rpy="${M_PI/2} 0 0"/>-->
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.072" radius="0.1265"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.59"/>
            <inertia ixx="1.416e-3" ixy="0.0" ixz="0.0" iyy="2.749e-3" iyz="0.0" izz="1.416e-3"/>
        </inertial>
    </link>


    <link name="left_back_wheel">
        <visual>
            <geometry>
                <cylinder length="0.072" radius="0.1265"/>
            </geometry>
            <!--            <origin xyz="0 0 0" rpy="${M_PI/2} 0 0"/>-->
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        <collision>
            <!--            <origin xyz="0 0 0" rpy="${M_PI/2} 0 0"/>-->
            <geometry>
                <cylinder length="0.072" radius="0.1265"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.59"/>
            <inertia ixx="1.416e-3" ixy="0.0" ixz="0.0" iyy="2.749e-3" iyz="0.0" izz="1.416e-3"/>
        </inertial>
    </link>

    <joint name="back_left_joint" type="continuous">
        <axis xyz="0 0 1"/>
        <parent link="base_link"/>
        <child link="left_back_wheel"/>
        <origin rpy="-1.57079 0 0" xyz="-0.4 0.35 -0.03"/>
        <limit effort="100" velocity="100"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

    <joint name="front_right_joint" type="continuous">
        <axis xyz="0 0 1"/>
        <parent link="base_link"/>
        <child link="right_front_wheel"/>
        <origin rpy="-1.57079 0 0" xyz="0.4 -0.35 -0.03"/>
        <limit effort="100" velocity="100"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

    <joint name="back_right_joint" type="continuous">
        <axis xyz="0 0 1"/>
        <parent link="base_link"/>
        <child link="right_back_wheel"/>
        <origin rpy="-1.57079 0 0" xyz="-0.4 -0.35 -0.03"/>
        <limit effort="100" velocity="100"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

    <joint name="front_left_joint" type="continuous">
        <axis xyz="0 0 1"/>
        <parent link="base_link"/>
        <child link="left_front_wheel"/>
        <origin rpy="-1.57079 0 0" xyz="0.4 0.35 -0.03"/>
        <limit effort="100" velocity="100"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

    <transmission name="left_front_wheel_trans" type="SimpleTransmission">
        <type>transmission_interface/SimpleTransmission</type>
        <actuator name="left_front_wheel_motor">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
        <joint name="front_left_joint">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
    </transmission>

    <transmission name="right_front_wheel_trans" type="SimpleTransmission">
        <type>transmission_interface/SimpleTransmission</type>
        <actuator name="right_front_wheel_motor">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
        <joint name="front_right_joint">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
    </transmission>

    <transmission name="left_back_wheel_trans" type="SimpleTransmission">
        <type>transmission_interface/SimpleTransmission</type>
        <actuator name="left_back_wheel_motor">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
        <joint name="back_left_joint">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
    </transmission>

    <transmission name="right_back_wheel_trans" type="SimpleTransmission">
        <type>transmission_interface/SimpleTransmission</type>
        <actuator name="right_back_wheel_motor">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
        <joint name="back_right_joint">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
    </transmission>
    <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <robotNamespace>/</robotNamespace>
            <legacyModeNS>true</legacyModeNS>
        </plugin>
    </gazebo>
</robot>